		"nodemon": "^3.0.1",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"express": "^4.18.2",
		"prettier": "^3.0.2",
		"swagger-ui-express": "^5.0.0",
		"ts-morph": "^19.0.0",
